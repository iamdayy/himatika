{
  "functions": {
    "source": ".output/server",
  "runtime": "nodejs20"
},
  "hosting": {
    "public": ".output/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls" : true,
    "rewrites": [
      {
        "source": "**",
        "function": "server" 
      }
    ]
  }
}
